# ImageCraft Pro - Browser-Only Configuration
# No server-side environment variables needed

# PWA Configuration
REACT_APP_NAME="ImageCraft Pro"
REACT_APP_SHORT_NAME="ImageCraft"
REACT_APP_DESCRIPTION="Modern PWA for image processing and editing"

# Build Configuration
GENERATE_SOURCEMAP=false
BUILD_PATH=build

# Development
REACT_APP_DEBUG=false

# File Processing Limits (Browser-Side)
REACT_APP_MAX_FILE_SIZE=100000000
REACT_APP_MAX_FILES_PER_UPLOAD=20
REACT_APP_SUPPORTED_FORMATS=jpeg,jpg,png,webp,avif,gif,bmp

# Processing Settings
REACT_APP_MAX_RESOLUTION=8192
REACT_APP_DEFAULT_QUALITY=80

# PWA Features
REACT_APP_ENABLE_OFFLINE_MODE=true
REACT_APP_ENABLE_PUSH_NOTIFICATIONS=false

# Authentication (Clerk)
REACT_APP_CLERK_PUBLISHABLE_KEY=pk_test_your_publishable_key